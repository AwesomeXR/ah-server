{
  "name": "ah-server",
  "version": "5.2.4",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "LOCAL_PORT=20000 nodemon -e ts --watch src --watch __test__ __test__/run.ts",
    "test": "LOCAL_PORT=20000 jest",
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit && npm run test",
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "release": "npm run ci && npm run build && rm -rf dist/* && npm run build && release-it"
  },
  "dependencies": {
    "@types/cron": "^1.7.2",
    "@types/jest": "^26.0.16",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.11.6",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^15.11.10",
    "@types/uuid": "^8.3.0",
    "ah-api-type": "^3.0.7",
    "ah-logger": "^3.2.5",
    "cron": "^1.8.2",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-router": "^9.4.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.3",
    "urllib": "^2.36.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^2.0.5",
    "prettier": "^3.2.3",
    "release-it": "^17.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
